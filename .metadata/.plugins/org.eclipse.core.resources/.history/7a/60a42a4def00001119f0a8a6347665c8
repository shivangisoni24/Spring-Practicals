	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="payment" class="com.rays.ioc.Payment">
		<property name="balance" value="500"></property>
	</bean>

	<bean id="inventry" class="com.rays.ioc.Inventry">
		<property name="stock" value="10"></property>
	</bean>

	<bean id="order" class="com.rays.ioc.Order1">
		<property name="payment" ref="payment"></property>
		<property name="inventry" ref="inventry"></property>
	</bean>

